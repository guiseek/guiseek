          "options": {
            "browserTarget": "samples-ngpeer-client:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/samples/ngpeer-client/src/**/*.ts"]
          }
        },
        "test": {
