              "apps/samples/ngpeer-client/src/assets"
            ],
            "styles": ["apps/samples/ngpeer-client/src/styles.scss"],
            "allowedCommonJsDependencies": ["socket.io-client"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/samples/ngpeer-client/src/environments/environment.ts",
                  "with": "apps/samples/ngpeer-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
