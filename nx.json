{
  "npmScope": "guiseek",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "ngpeer-core": {
      "tags": ["side:common", "type:util"]
    },
    "samples-ngpeer-server": {
      "tags": ["side:server", "type:app"]
    },
    "ngpeer-server": {
      "tags": ["side:server"]
    },
    "ngpeer-client": {
      "tags": ["side:client", "type:feature"]
    },
    "samples-ngpeer-client": {
      "tags": ["side:client", "type:app"]
    },
    "samples-ngpeer-client-e2e": {
      "tags": [],
      "implicitDependencies": ["samples-ngpeer-client"]
    },
    "peer-core": {
      "tags": ["type:util", "scope:peer"]
    },
    "peer-client": {
      "tags": ["side:client", "type:feature", "scope:peer"]
    },
    "sample-peer": {
      "tags": ["side:client", "type:app", "scope:sample"]
    },
    "sample-peer-e2e": {
      "tags": [],
      "implicitDependencies": ["sample-peer"]
    },
    "sample-gateway": {
      "tags": ["side:server", "type:app", "scope:sample"]
    },
    "peer-player": {
      "tags": ["side:client", "type:feature", "scope:peer"]
    }
  }
}
